@model IEnumerable<Utente>
@{
    ViewData["Title"] = "Index";
}

<table class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>
                Category Name
            </th>
            <th>
                Display Order
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td width="50%">
                    @obj.Nome
                </td>
                <td with="30%">
                    @obj.Cognome
                </td>
            </tr>
        }
    </tbody>
</table>

